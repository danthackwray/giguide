<div class="container-venue-show">
  <div class="venue-info">
    <%= render @venue %>
    # <%= cl_image_tag @venue.photo.key %>
    <div class="btns-venue-show">
      <%= link_to "All venues", venues_path, class:"venue-button"  %>
      <% if @venue.user == current_user %>
        <%= link_to "Edit this venue", edit_venue_path(@venue), class:"venue-button" %>
        <%= button_to "Delete this venue", @venue, method: :delete, class:"venue-button"  %>
      <% end %>
    </div>
  </div>
</div>
